<?xml version="1.0"?>
<package>
  <metadata>
    <id>TinymceDamPicker</id>
    <version>0.4.20</version>
    <title>DAM picker button for TinyMCE</title>
    <authors>David Knipe</authors>
    <owners>David Knipe</owners>
    <icon>dam-select-icon.png</icon>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      Enables editors to pick images from the Optimizely DAM in in the TinyMCE rich-text editor
    </description>
    <repository type="git" url="https://github.com/davidknipe/TinymceDamPicker.git"></repository>
    <releaseNotes>
      Compatible with CMS 12 and EPiServer.CMS.TinyMce 3.x.
    </releaseNotes>
    <tags>EPiServerModulePackage EPiServerAddOn TinyMCE DAM</tags>
    <dependencies>
      <group targetFramework="net6.0">
        <dependency id="EPiServer.CMS.TinyMce" version="[3.0.1,)" />
        <dependency id="Microsoft.Extensions.FileProviders.Embedded" version="[6.0.5,)" />
      </group>
    </dependencies>
    <contentFiles>
      <files include="any/any/modules/_protected/TinymceDamPicker/plugin.js" buildAction="None" copyToOutput="false"  />
      <files include="any/any/modules/_protected/TinymceDamPicker/module.config" buildAction="None" copyToOutput="false"  />
    </contentFiles>
  </metadata>
  <files>
    <file src="..\..\TinymceDamPicker\bin\Release\net6.0\TinymceDamPicker.dll" target="lib\net6.0\" />
    <file src="..\..\TinymceDamPicker\ClientResources\tinymcedampicker\plugin.js" target="contentFiles\any\any\modules\_protected\TinymceDamPicker" />
    <file src="..\..\TinymceDamPicker\module.config" target="contentFiles\any\any\modules\_protected\TinymceDamPicker" />
    <file src="docs\dam-select-icon.png" />
  </files>
</package>